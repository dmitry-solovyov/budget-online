@model BudgetOnline.Web.ViewModels.Charts.TagsByDatesViewModel
@{
}
@section Header
{
    <script src="@Url.Content("~/Scripts/Highcharts-3.0.1/js/highcharts.js")" type="text/javascript"></script>
}
<div class="darkback border-bt" style="padding-bottom: 1em">
    <form method="POST">
    <div class="row">
        <div class="col-md-3">
            <strong>Период</strong>
        </div>
        <div class="col-md-9">
            <div class="row">
                <div class="col-md-3">@Html.EditorFor(model => model.FromDate)</div>
                <div class="col-md-3">@Html.EditorFor(model => model.ToDate)</div>
                <div class="col-md-6">
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <strong>Тэги</strong>
        </div>
        <div class="col-md-9">
        </div>
    </div>
    <div class="row">
        <div class="col-md-9 col-md-offset-3">
            @Html.BudgetOnlineWeb().ButtonNormal().Caption("Выбрать").Build()
            <span style="padding-left: 3em">
                @Html.EditorFor(model => model.Tags)
            </span>
        </div>
    </div>
    </form>
</div>
<div class="row">
    <div class="col-md-12">
        @(Model.Chart)
    </div>
</div>
