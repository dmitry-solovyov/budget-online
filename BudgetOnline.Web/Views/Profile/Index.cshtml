@using BudgetOnline.Web.UI.Controls
@{
	var route = ViewContext.RequestContext.RouteData;
}
@model BudgetOnline.Web.ViewModels.ProfileViewModel
@{
	ViewBag.Title = "Profile";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
@(Html.BudgetOnlineWeb().Form(BudgetOnlineWebControls.FormTypes.Horizontal)
	.ActionUrl(Url.Action(route.GetRequiredString("action"), route.GetRequiredString("controller")))
	.Content(
		@<text>
@Html.EditorForModel()
</text>)
	.Build())
